<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.shinyuarticle.mapper.UserMapper">
    <select id="getAllUser" resultType="com.example.shinyuarticle.bean.UmsMember">
        select * from ums_member
    </select>

    <select id="getUserById" resultType="com.example.shinyuarticle.bean.UmsMember">
        select * from ums_member where id=#{id}
    </select>



</mapper>